<h1> <%= @car.brand %> </h1>
<h2> <%= @car.model %> </h2>
<p> <%= @car.fuel %>
<p> <%= @car.year %>
<hr>
<%= simple_form_for [@car, @review] do |f| %>
<%= f.input :comment %>
<%= f.input :rating, collection: 1..5 %>
<%= f.submit %>
<% end %>
<hr>
<% @reviews.each do |review| %>
<p><%= review.comment %> </p>
<p><%= '' * review.rating %> </p>
<% end %>
